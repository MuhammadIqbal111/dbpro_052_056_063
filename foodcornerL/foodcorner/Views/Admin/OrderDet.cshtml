@model IEnumerable<foodcorner.Models.PlaceOrder>

@{
    ViewBag.Title = "OrderDet";
}

<h2>Index</h2>

<p style="margin-top:50px;font-family:Arial;font-size:15px; margin-bottom:25px;padding-top:70px;padding-left:30px; text-align:left"><b> Delivery Teams</b></p>

<table class="table">
    <tr>
        <th style="font-family:Arial;font-size:24px">
            @Html.DisplayNameFor(model => model.ItemsDetail.Name)
        </th>
        <th style="font-family:Arial;font-size:24px">
            @Html.DisplayNameFor(model => model.quantity)

        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td style="font-family:Arial;font-size:24px">
                @Html.DisplayFor(modelItem => item.ItemsDetail.Name)
            </td>
            <td style="font-family:Arial;font-size:24px">
                @Html.DisplayFor(modelItem => item.quantity)
            </td>

        </tr>
    }

</table>
